<view id='QRcode'>
  <view>
    <view class='address'>出示二维码核销</view>
    <view class='QRcode-image'>
      <image src='{{qrUrl}}'></image>
    </view>
    <view class="phoneStr">
        <view>{{phoneStr}}</view>
    </view>
    <view class='address-box' wx:if="{{address}}">
      <view class="address-info" bindtap="toAddress" data-info="{{address}}">
        <view class="title">{{name || ''}}</view>
        <view class="con">{{address.address|| ''}}<view class="showmap" wx:if="{{address.coordinate}}">查看地图</view></view>
      </view>
      <view class="address-phone" wx:if="{{address.phone}}" bindtap="mackPhoneCall" data-phone="{{address.phone}}">
      <i class="iconfont icon-shoujihao"></i>
      <span>电话</span>
      </view>
    </view>
    <view class='Technical-support'>么么校园</view>
  </view>
</view>
