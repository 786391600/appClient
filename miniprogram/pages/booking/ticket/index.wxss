@import '../../../style/ticket/iconfont.wxss';
.ticket-page{
  padding: 10rpx;
  box-sizing: border-box;
}
.official-account {
  margin-bottom: 15rpx;
}
.ticket4{
  font-size: 40rpx;
}
.ticket-box {
  background: white;
  padding: 19rpx;
  box-sizing: border-box;
  border-radius: 10rpx;
}
.ticket-top {
  display: flex;
  flex-direction: row;
}
.ticket-top-left-input {
  display: flex;
  flex-direction: row;
  border-bottom: 1rpx dashed #EFEFEF;
  align-items: center;
}
.ticket-top-left-input i {
  font-size: 50rpx;
}
.ticket-top-left-input .ticket1 {
  color: #FCDC6D;
}
.ticket-top-left-input .ticket2 {
  color: #80D3F3;
}
.ticket-top-left-input view{
  padding: 30rpx;
  box-sizing: border-box;
  color: #33383E;
  font-weight: bold;
}
.ticket-top-left {
  width: 72%;
}
.ticket-top-right {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 28%;
}
.ticket-btn {
  margin-top: 19rpx;
}
.ticket-btn button {
  background: #FED349;
  color: #825119;
}
.ticket-top-right .trans-btn {
  transform: rotate(90deg);
  border: 3rpx solid #EBEBEB;
  background-color: var(--var-color1);
  border-radius: 50%;
  color: #B9B9B9;
  padding: 20rpx;
}
.HistoricalRecord{
  padding: 16rpx 30rpx;
  background: #ffffff;
  margin-top: 30rpx;
  border-radius:10rpx;
  position: relative;
}
.del{
  position: absolute;
  right: 15rpx;
  top: 15rpx;
  color: #c1bfcc;
  font-size: 50rpx;
}
.Histori-title {
  text-align: center;
  height: 50rpx;
  margin-top: 10rpx;
}

.HistoricalRecord-title {
  display: table;
  width: 100%;
  line-height: 50rpx;
  white-space: nowrap;
  border-spacing: 2rem 0;
  font-size: 35rpx;
  color: #bad4f5
}
.HistoricalRecord-title::before, .HistoricalRecord-title::after {
  display: table-cell;
  content: "";
  width: 50%;
  background: linear-gradient(#bad4f5, #bad4f5) repeat-x center;
  background-size: 0.1rem 0.1rem;
}
.HistoricalRecord-title2 {
  display: table;
  width: 100%;
  line-height: 50rpx;
  white-space: nowrap;
  border-spacing: 2rem 0;
  font-size: 35rpx;
  color: #eddd86
}
.HistoricalRecord-title2::before, .HistoricalRecord-title2::after {
  display: table-cell;
  content: "";
  width: 50%;
  background: linear-gradient(#eddd86, #eddd86) repeat-x center;
  background-size: 0.1rem 0.1rem;
}
.recommend-title {
  display: table;
  width: 100%;
  line-height: 50rpx;
  white-space: nowrap;
  border-spacing: 2rem 0;
  font-size: 35rpx;
  color: #fde58d
}
.recommend-title::before, .recommend-title::after {
  display: table-cell;
  content: "";
  width: 50%;
  background: linear-gradient(#ffe991, #ffe991) repeat-x center;
  background-size: 0.1rem 0.1rem;
}
.Histori-content{
  padding-left:60rpx;
  overflow: hidden;
}
.Histori-content view{
  font-size: 30rpx;
  margin-top: 10rpx;
  width: 50%;
  float: left;
}
.eg2-box{
  width: 100%;
  padding: 10rpx 5rpx 5rpx 5rpx;
  background: #f8e183;
  border-radius: 10rpx;
  margin-bottom: 20rpx;
  margin-top: 10rpx;
}
.eg2{
  width: 100%;
  height: 67rpx;
  background: #fcf9ce;
  position: relative;
  border-bottom-left-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
}
.circle1 {
        position: absolute;
        z-index: 5;
        top: -6rpx;
        left: 0;
        transform-origin: center left;
        transform: rotate(0deg);
        width: 100%;
        height: 9px;
        background-image: radial-gradient(circle, #fcf9ce, #fcf9ce 4px, transparent 0px, transparent 2px, transparent 22px, transparent);
        background-size: 10px 10px;
        background-repeat: repeat-x;
    }
    .Recommend-content{
      margin-left: 20rpx;
      font-size: 28rpx;
      line-height: 67rpx;
      color: #8c3d2e;
    }
    .Recommend-monery{
      position: absolute;
      right: 15rpx;
      top: 0;
      line-height: 67rpx;
      font-size: 28rpx;
      color: #be0005;
    }
    .Edition{
      font-size: 32rpx;
      color: #D4D4D4;
      margin: 30rpx 0 5rpx;
      text-align: center;
      font-weight: bold;
      font-style: ï¼›
    }


    .share{
      position: fixed;
      left: 0rpx; 
      bottom: 300rpx;
      height: 60rpx;
      /* overflow: hidden; */
      /* font-size: 44rpx; */
    }
    .share .i {
      font-size: 34rpx;
    }
    .share button{
      width: 100%;
      height: 100%;
      display: flex;
      align-items: center;
      justify-content: center;
      border: none;
      outline: none;
      background-color: #FFD148;
      color: #784A00;
      opacity: 0.8;
      border-radius: 0%;
      border-top-right-radius: 40rpx;
      border-bottom-right-radius: 40rpx;
      border: 1rpx solid #DFDFDF;
      font-size: 24rpx;
      font-weight: bold;
    }
    .share button::after{
      border: none;
    } 
