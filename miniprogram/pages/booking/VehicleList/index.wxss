/* miniprogram/pages/VehicleList/index.wxss */
page {
  background: #eeeeee;
}
#PageTurning{
  position: fixed;
  top: 0;
  width: 100%;
  display: flex;
  justify-content: space-between;
  background: #fff;
  padding: 25rpx 12rpx;
  box-sizing:border-box;
  border-bottom: #eee 1rpx solid;
  font-size: 30rpx;
  font-weight: 700;
}
#PageTurning view{
  color: #03a9f4
}
#ListBackground{
  padding:0  0 90rpx;
}
.TitleAddress{
  text-align: center;
  padding: 20rpx 0;
  font-size: 30rpx;
  color: #949494;
}
.ContentAddress{
  padding: 0 10rpx;
  margin-bottom: 24rpx;
}
.ContentBox{
  background: #ffffff;
}
.BoxTitle{
  padding: 26rpx 28rpx;
  display: flex;
  justify-content: space-between;
}
.BoxTitle view:nth-child(1){
  font-size: 26rpx;
}
.BoxTitle view:nth-child(2){
  color: #f5693c;
  font-size: 32rpx;
  font-weight: bold;
}
.BoxContent{
  padding: 0rpx 0rpx 28rpx 40rpx;
  display: flex;
  justify-content: space-between;
}
.context-box{
  overflow: hidden;
  width: 80%;
}
.cont-backg{
  width: 100%;
  position: relative;
  overflow: hidden;
  margin-top: 22rpx;
  display: flex;
  align-items: center;
}
.context-box .cont-backg view{
    float: left;
}
.context-box .cont-backg view:nth-child(1){
  color: #ffffff;
  border-radius: 50%;
  font-size: 21rpx;
  width: 32rpx;
  height: 32rpx;
  line-height: 32rpx;
  text-align: center;
}
.context-box .cont-backg view:nth-child(2){
  margin-left: 12rpx;
  font-size: 30rpx;
  width: 75%;
}
.context-box .cont-backg view:nth-child(3){
  font-size: 24rpx;
  color: #f44f1c;
  position: absolute;
  right: 0
}
.distance{
  width: 30%;
  display: flex;
  align-items: center;
}
.distance view:nth-child(1){
  float: left;
  font-size: 26rpx;
  color: #03a9f4;
  border: 1rpx #03a9f4 solid;
  border-radius:25rpx; 
  margin: 16rpx 0 0 30rpx;
  padding: 8rpx 16rpx;
}
.provider{
  border-top: 1rpx #ececec solid;
  padding: 26rpx 28rpx;
  font-size: 32rpx;
}