@import '../../../style/ticket/iconfont.wxss';
.resumeBox{
  padding: 40rpx;
}
.resumeBox-info{
  background: white;
  border-radius: 25rpx;
}
.resumeBox-info-top{
  padding: 40rpx;
  border-bottom: 2rpx solid #E6E6E6;
}
.resumeBox-info-bottom{
  padding: 75rpx;
}
.resumeBox-info-bottom > view {
  color: #868686;
  font-size: 32rpx;
  margin: 18rpx 0 18rpx;
}
.resumeBox-info-bottom .name{
  font-size: 38rpx;
  color: black;
}
.tip {
  text-align: center;
  color: #919191;
  margin: 350rpx auto 0;
}
.tip .iconfont {
  font-size: 100rpx;
}
.tip button{
  width: 60%;
  height: 90rpx;
  background: #FF783B;
  border-radius: 15rpx;
  color: white;
  font-size: 42rpx;
  line-height: 80rpx;
  margin-top: 25rpx;
}
.iconfont{
  color: #FF783B;
  margin-right: 15rpx;
}
