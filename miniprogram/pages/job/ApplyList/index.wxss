@import '../../../style/ticket/iconfont.wxss';
.ApplyList {
  padding: 40rpx;
  box-sizing: border-box;
}
.ApplyBox{
  background: white;
  border-radius: 20rpx;
}
.ApplyBox-top{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.ApplyBox-top{
  padding: 20rpx 38rpx 20rpx;
  border-bottom: 2rpx solid #E8E8E8;
}
.ApplyBox-bottom{
  padding: 20rpx 38rpx 20rpx;
}
.ApplyBox-bottom > view{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin: 10rpx 0 10rpx;
}
.ApplyBox-bottom .price {
  color: #EF8700;
  font-size: 32rpx;
}
.ApplyBox-bottom .company{
  font-size: 32rpx;
}
.ApplyBox-bottom .place{
  font-size: 32rpx;
  color: #777777;
}
.position{
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 36rpx;
  width: 70%;
}